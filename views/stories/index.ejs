<%- include("../partials/header") -%>

<div class="container">
    <h1>View the stories for the <%= contest.title %> contest here!</h1>
    <div class="row">
        <% stories.forEach(function(story) { %>
        <div class="col-12 col-lg-6">
            <%- include("story-card", { story: story }) -%>
        </div>
        <% }); %>
    </div>
</div>

<%- include("../partials/footer") -%>